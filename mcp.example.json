{
  "servers": {
    "built-in-tester": {
      "type": "stdio",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "mcp-servers/mcp-tester/mcp-tester.csproj"
      ],
      "alwaysAllow": [
        "echo",
        "current-time"
      ]
    },
    "cloudflare_demo_day": {
      "type": "http",
      "endpoint": "https://demo-day.mcp.cloudflare.com/sse"
    }
  }
}