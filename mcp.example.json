{
  "inputs": [],
  "servers": {
    "built-in-tester": {
      "type": "stdio",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "mcp-servers/mcp-tester/mcp-tester.csproj"
      ],
      "alwaysAllow": [
        "echo",
        "current-time"
      ]
    },
    "external-server": {
      "type": "stdio", 
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-everything"
      ]
    }
  }
}